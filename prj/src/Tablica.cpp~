 * Plik zawiera metody klasy Benchmarker.
 */

#include "Benchmark.hh"

#include <cstdlib>
#include <fstream>
#include <ctime>

/*!
 *\brief Inicjalizator .
 *\param
 * polozenie- typu Punkt, punkt poczatkowy manipulatora.
 *\param
 * nazwa - typu string, nazwa manipulatora.
 */
Benchmark::Benchmark(long double* seria)
{}

/*!
 *\brief Destruktor bezparametryczny.
 */
Benchmark::~Benchmark()
{}

Benchmark::Testuj(Tablica tab,int* dane,int liczba_przejsc,int liczba_danych);
  {
  long double czas_operacji;
  struct timespec start,stop;

    czasy=new long double[liczba_przejsc];

    tab.wypelnij_tablice(dane,liczba_danych);

    clock_gettime(CLOCK_REALTIME, &start);

    for(int i;i<liczba_danych;i++)
      {
	tab.element[i]=tab.element[i]*2;
      }  
    clock_gettime(CLOCK_REALTIME, &stop);

    czas_operacji = (stop.tv_nsec - start.tv_nsec);
    
  }
